
@{
    @model InvestigateCreateViewModel
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Index"; }

<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-flex align-items-center justify-content-between">
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Tables</a></li>
                    <li class="breadcrumb-item active">Incident Reports</li>
                </ol>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <div class="row mb-5">
                    <div class="col-md-10">
                        <h4>Investigate/Incident</h4>
                    </div>
                    <div class="col-md-2">
                        <div class="dropdown">
                            <button style="width:100%; background:#b81322;border:1px solid #b81322" class="btn btn-secondary dropdown-toggle"
                                    type="button" id="dropdownMenuButton" data-toggle="dropdown"
                                    aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-newspaper"></i>
                                Operation <i class="mdi mdi-chevron-down"></i>
                            </button>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                @if (User.IsInRole("User"))
                                {
                                    <a class="dropdown-item itemAdd" href="#" data-toggle="modal"
                                       data-target=".bs-example-modal-xl">Create New</a>
                                    <div class="dropdown-divider"></div>
                                    @foreach (var m in ViewBag.InvestList)
                                    {
                                        @if (m.ConfirmStatus == 0)
                                        {
                                            <a href="javascript:;" class="dropdown-item itemClick">Edit Data</a>
                                        }
                                    }

                                    <div class="dropdown-divider"></div>

                                    <a href="javascript:;" class="dropdown-item itemDel">Delete Data</a>
                                    <div class="dropdown-divider"></div>

                                }
                                @if (User.IsInRole("Admin"))
                                {
                                    <a class="dropdown-item itemAdminAdd" href="#" data-toggle="modal"
                                       data-target=".adminModalLabel">Create new</a>
                                    <div class="dropdown-divider"></div>

                                    @foreach (var m in ViewBag.IncidentList)
                                    {
                                        @if (m.ConfirmStatus == 0)
                                        {
                                            <a href="javascript:;" class="dropdown-item itemAdminClick">Edit Data</a>
                                        }
                                    }

                                    <div class="dropdown-divider"></div>

                                    <a href="javascript:;" class="dropdown-item itemClick">Edit User's Data</a>
                                    <div class="dropdown-divider"></div>

                                    <a href="javascript:;" class="dropdown-item itemAdminDel">Delete Data</a>
                                    <div class="dropdown-divider"></div>

                                    <a href="javascript:;" class="dropdown-item itemPrint">
                                        Incident reports index
                                    </a>
                                }
                                @if (User.IsInRole("SuperAdmin"))
                                {
                                    <a href="javascript:;" class="dropdown-item itemClick">Edit User's Data</a>
                                    <div class="dropdown-divider"></div>
                                    <a href="javascript:;" class="dropdown-item itemAdminClick">Edit Data</a>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                @if (User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
                {
                    <table id="datatable-buttons"
                           class="table  display table-striped table-bordered dt-responsive nowrap clicks-table"
                           style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th style="display:none"></th>
                                <th> Cause of the occurrence </th>
                                <th>Outcome for people</th>
                                <th>Outcome for property </th>
                                <th>Outcome for environment </th>
                                <th>Outcome for business </th>
                                <th>Possible outcome for people </th>
                                <th>Possible outcome for property</th>
                                <th>Possible outcome for environment</th>
                                <th>Possible outcome for business</th>
                                <th> Lessons Learned </th>
                                <th> Brief summary of investigation</th>
                                <th>Explain the reasons root cause</th>
                                <th> Identify the root cause</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var m in ViewBag.IncidentList)
                            {
                                <tr @{if (m.ConfirmStatus == 0) { <text> style="background-color: #e5a126; color:#fff " </text>  } else { <text> style="background-color: #99d699; color:#fff" </text>  } } data-id="@m.Id">
                                    <td></td>
                                    <td>@m.OccurenceCause.Name</td>
                                    <td>@m.OutcomePeople.Name</td>
                                    <td>@m.OutcomeProperty.Name</td>
                                    <td>@m.OutcomeEnviroment.Name</td>
                                    <td>@m.OutcomeBuisness.Name</td>
                                    <td>@m.PossibleBuisness.Name</td>
                                    <td>@m.PossibleProperty.Name</td>
                                    <td>@m.PossibleEnviroment.Name</td>
                                    <td>@m.PossiblePeople.Name</td>
                                    <td>@m.LessonLearned.Name</td>
                                    <td>@m.BriefDecription</td>
                                    <td>@m.ReasonDescription</td>
                                    <td>@m.IdentifyDescription</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
                <hr style="border: 1px solid #d60707;" />

                @if (User.IsInRole("User") || User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
                {
                    <table id="datatable-buttons"
                           class="table display table-striped table-bordered dt-responsive nowrap clicked-table"
                           style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th style="display:none"></th>
                                <th>
                                    Name of reporter
                                </th>
                                <th>Position of reporter</th>
                                <th>
                                    Date of occurrence
                                </th>
                                <th>
                                    Area & address where incident occurred:
                                </th>
                                <th>
                                    Activity taking place at time of occurrence
                                </th>
                                <th>Occurrence Reason</th>
                                <th>Occurrence Type</th>
                                <th>Weather conditions </th>
                                <th>Ground type /cover</th>
                                <th>
                                    Ground condition
                                </th>
                                <th>
                                    Illumination - Lighting
                                </th>
                                <th>
                                    Type of contact
                                </th>
                                <th>
                                    Type of injury
                                </th>
                                <th>Apparent type of illness </th>
                                <th>Bodily location</th>
                                <th>
                                    Actions of injured person after the incident
                                </th>
                                <th>Release Type</th>
                                <th>Released To </th>
                                <th>
                                    Description of the incident
                                </th>
                                <th>
                                    Immediate action taken:
                                </th>
                            </tr>
                        </thead>
                        <tbody id="myColor">
                            @foreach (var m in ViewBag.InvestList)
                            {
                                <tr @{if (m.ConfirmStatus == 0) { <text> style="background-color: #e5a126; color:#fff " </text>  } else { <text> style="background-color: #99d699; color:#fff" </text>  } } data-id="@m.Id">
                                    <td></td>
                                    <td>@m.Name</td>
                                    <td>@m.Position</td>
                                    <td>@m.DateTime.ToString("yyyy-MM-dd")</td>
                                    <td>@m.Area</td>
                                    <td>@m.Activity</td>
                                    <td>@m.OccurenceReason.Name</td>
                                    <td>@m.OccurenceType.Name</td>
                                    <td>@m.WheatherType.Name</td>
                                    <td>@m.GroundType.Name</td>
                                    <td>@m.GroundConditionType.Name</td>
                                    <td>@m.IlluminationType.Name</td>
                                    <td>@m.ContactType.Name</td>
                                    <td>@m.InjuryType.Name</td>
                                    <td>@m.IlnessType.Name</td>
                                    <td>@m.BodilyLocationType.Name</td>
                                    <td>@m.InjuredActionType.Name</td>
                                    <td>@m.ReleaseType.Name</td>
                                    <td>@m.ReleasedToType.Name</td>
                                    <td>@m.DescriptionIncident</td>
                                    <td>@m.DescriptionImmediate</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row  || -->
@if (User.IsInRole("User") || User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
{
    <div class="modal fade bs-example-modal-xl ModalLoad" id="ModalHide" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel"
         aria-hidden="true">
        @*<div id="ajaxLoader-inner"></div>*@
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header" style="background: brown;">
                    <p style="color: #fff; font-size: 10px;">Incident_No_123</p>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true" style="color:#fff">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div>
                        <form method="post" id="investigate-form">
                            <div class="container">
                                <div class="row">
                                    <div class="col-xs-12" style="width:100%">
                                        <nav>
                                            <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                                                <a class="nav-item nav-link active" id="nav-home-tab" style="color: brown;" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">Incident</a>
                                                <a class="nav-item nav-link" id="nav-profile-tab" style="color: brown;" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">Occurence</a>
                                                <a class="nav-item nav-link" id="nav-contact-tab" style="color: brown;" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Material</a>
                                                <a class="nav-item nav-link" id="nav-about-tab" style="color: brown;" data-toggle="tab" href="#nav-about" role="tab" aria-controls="nav-about" aria-selected="false">Image</a>
                                            </div>
                                        </nav>
                                        <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent">
                                            <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
                                                @*   Begin Investigate*@
