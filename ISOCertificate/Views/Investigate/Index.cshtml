                                                        <tbody id="line-body">
                                                        </tbody>
                                                    </table>
                                                </div>
                                                @*End InvestLine*@
                                            </div>
                                            <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
                                                @*Begin Material*@
                                                <div class="row mt-4">
                                                    <div class="col-12">
                                                        <h3 style="padding-bottom: 26px;">Material Information</h3>
                                                    </div>
                                                    <div class="col-md-2 col-12">
                                                        <div class="form-group">
                                                            <input class="form-control" type="hidden" id="material-id" value="0">
                                                            <label>Material Release  <span style="color:red;"> * </span></label>
                                                            <select class="form-control" id="material-release" asp-items="@(new SelectList(ViewBag.MaterialRelease, "Id", "Name"))" style="width:100%; padding: 10px;">
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-2 col-12">
                                                        <div class="form-group">
                                                            <label>Material Category  <span style="color:red;"> * </span></label>
                                                            <select class="form-control" id="material-category" asp-items="@(new SelectList(ViewBag.MaterialCategory, "Id", "Name"))" style="width:100%; padding: 10px;">
