                                                        <div class="form-group">
                                                            <label>Unsafe Fact  <span style="color:red;"> * </span></label>
                                                            <select class="form-control" id="inv-safeFact" asp-items="@(new SelectList(ViewBag.UnSafeFactType, "Id", "Name"))" multiple style="width:100%; padding: 5px;">
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2 col-12">
                                                        <div class="form-group">
                                                            <label>UnSafe Condition  <span style="color:red;"> * </span></label>
                                                            <select class="form-control" id="inv-safeCondition" asp-items="@(new SelectList(ViewBag.UnSafeConditionType, "Id", "Name"))" multiple style="width:100%; padding: 5px;">
                                                            </select>
                                                        </div>
                                                    </div>
