                                                            @*<input class="form-control" type="text" placeholder="Immedity" id="inv-descriptionImmediate">*@
                                                        </div>
                                                    </div>

                                                    <div class="col-md-2 col-12">
                                                        <div class="form-group">
                                                            <label id="lbl_error_investigate" style="color:red;"></label>
